{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous", "authenticated"],
      "headers": {
        "Access-Control-Allow-Headers": "Authorization"
      }
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "auth": {
    "identityProviders": {
      "customOpenIdConnectProviders": []
    }
  }
}
